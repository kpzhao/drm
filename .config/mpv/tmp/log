[   0.417][d][vo/gpu/opengl] [ 42] weights[4] = c[0];
[   0.417][d][vo/gpu/opengl] [ 43] weights[5] = c[1];
[   0.417][d][vo/gpu/opengl] [ 44] // scaler samples
[   0.417][d][vo/gpu/opengl] [ 45] c = texture(tex, base + pt * vec2(0.0));
[   0.417][d][vo/gpu/opengl] [ 46] color += vec4(weights[0]) * c;
[   0.417][d][vo/gpu/opengl] [ 47] c = texture(tex, base + pt * vec2(1.0));
