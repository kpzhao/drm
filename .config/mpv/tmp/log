[   0.326][d][vo/gpu/opengl] [ 23] vec4 color = vec4(0.0, 0.0, 0.0, 1.0);
[   0.326][d][vo/gpu/opengl] [ 24] color.r = 1.000000 * vec4(texture(texture0, texcoord0)).r;
[   0.326][d][vo/gpu/opengl] [ 25] color.gb = 1.000000 * vec4(texture(texture1, texcoord1)).rg;
[   0.326][d][vo/gpu/opengl] [ 26] color = color.rgbr;
[   0.326][d][vo/gpu/opengl] [ 27] color.rgb = mat3(colormatrix) * color.rgb + colormatrix_c;
[   0.326][d][vo/gpu/opengl] [ 28] color.a = 1.0;
[   0.326][d][vo/gpu/opengl] [ 29] // scaler pre-conversion
[   0.326][d][vo/gpu/opengl] [ 30] // linearize
[   0.326][d][vo/gpu/opengl] [ 31] color.rgb = clamp(color.rgb, 0.0, 1.0);
[   0.326][d][vo/gpu/opengl] [ 32] color.rgb = pow(color.rgb, vec3(2.4));
[   0.326][d][vo/gpu/opengl] [ 33] color.rgb *= vec3(1.0/1.000000);
[   0.326][d][vo/gpu/opengl] [ 34] color.rgb = clamp(color.rgb, 0.0, 1.0);
[   0.326][d][vo/gpu/opengl] [ 35] color.rgb = 0.750000 - log(1.0/(color.rgb * 0.827906 + 0.007577) - 1.0) * 1.0/6.500000;
[   0.326][d][vo/gpu/opengl] [ 36] // main scaling
[   0.326][d][vo/gpu/opengl] [ 37] out_color = color;
[   0.326][d][vo/gpu/opengl] [ 38] }
