[   0.326][d][vo/gpu/opengl] [ 10] layout(offset=48) vec3 colormatrix_c;
[   0.326][d][vo/gpu/opengl] [ 11] layout(offset=64) vec2 texture_size0;
[   0.326][d][vo/gpu/opengl] [ 12] layout(offset=80) mat2 texture_rot0;
