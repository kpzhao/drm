[   0.417][d][vo/gpu/opengl] [ 67] color.rgb *= vec3(1.000000);
[   0.417][d][vo/gpu/opengl] [ 68] color.rgb = pow(color.rgb, vec3(1.0/2.4));
[   0.417][d][vo/gpu/opengl] [ 69] // dithering
