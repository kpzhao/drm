[   0.417][d][vo/gpu/opengl] [ 55] c = texture(tex, base + pt * vec2(5.0));
[   0.417][d][vo/gpu/opengl] [ 56] color += vec4(weights[5]) * c;
[   0.417][d][vo/gpu/opengl] [ 57] }
[   0.417][d][vo/gpu/opengl] [ 58] color.a = 1.000000;
[   0.417][d][vo/gpu/opengl] [ 59] // scaler post-conversion
[   0.417][d][vo/gpu/opengl] [ 60] color.rgb = clamp(color.rgb, 0.0, 1.0);
[   0.417][d][vo/gpu/opengl] [ 61] color.rgb = (1.0/(1.0 + exp(6.500000 * (0.750000 - color.rgb))) - 0.007577) * 1.0/0.827906;
