[   0.176][d][vo/gpu/opengl] [ 40] vec4 average(float range, inout float h) {
[   0.176][d][vo/gpu/opengl] [ 41] float dist = rand(h) * range; h = permute(h);
[   0.176][d][vo/gpu/opengl] [ 42] float dir = rand(h) * 6.2831853; h = permute(h);
[   0.176][d][vo/gpu/opengl] [ 43] vec2 o = dist * vec2(cos(dir), sin(dir));
