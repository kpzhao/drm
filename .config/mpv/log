[   0.139][d][vo/gpu/opengl] [ 14] uniform sampler2D texture0;
[   0.139][d][vo/gpu/opengl] [ 15] #define HOOKED_raw texture0
[   0.139][d][vo/gpu/opengl] [ 16] #define HOOKED_pos texcoord0
[   0.139][d][vo/gpu/opengl] [ 17] #define HOOKED_size texture_size0
