[   0.283][d][vo/gpu/opengl] [ 42] float dir = rand(h) * 6.2831853; h = permute(h);
[   0.283][d][vo/gpu/opengl] [ 43] vec2 o = dist * vec2(cos(dir), sin(dir));
[   0.283][d][vo/gpu/opengl] [ 44] vec4 ref[4];
[   0.283][d][vo/gpu/opengl] [ 45] ref[0] = HOOKED_texOff(vec2( o.x, o.y));
