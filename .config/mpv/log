[   0.193][d][vo/gpu/opengl] [ 26] color = color.rgbr;
[   0.193][d][vo/gpu/opengl] [ 27] color.rgb = mat3(colormatrix) * color.rgb + colormatrix_c;
[   0.193][d][vo/gpu/opengl] [ 28] color.a = 1.0;
[   0.193][d][vo/gpu/opengl] [ 29] // scaler pre-conversion
[   0.193][d][vo/gpu/opengl] [ 30] // linearize
[   0.193][d][vo/gpu/opengl] [ 31] color.rgb = clamp(color.rgb, 0.0, 1.0);
[   0.193][d][vo/gpu/opengl] [ 32] color.rgb = pow(color.rgb, vec3(2.4));
[   0.193][d][vo/gpu/opengl] [ 33] color.rgb *= vec3(1.0/1.000000);
[   0.193][d][vo/gpu/opengl] [ 34] color.rgb = clamp(color.rgb, 0.0, 1.0);
[   0.193][d][vo/gpu/opengl] [ 35] color.rgb = 0.750000 - log(1.0/(color.rgb * 0.827906 + 0.007577) - 1.0) * 1.0/6.500000;
[   0.193][d][vo/gpu/opengl] [ 36] // main scaling
[   0.193][d][vo/gpu/opengl] [ 37] out_color = color;
[   0.193][d][vo/gpu/opengl] [ 38] }
