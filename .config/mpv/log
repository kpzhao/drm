[   0.136][d][vo/gpu/opengl] [ 35] float mod289(float x) { return x - floor(x * 1.0/289.0) * 289.0; }
[   0.136][d][vo/gpu/opengl] [ 36] float permute(float x) {
[   0.136][d][vo/gpu/opengl] [ 37] return mod289( mod289(34.0*x + 1.0) * (fract(x) + 1.0) );
[   0.136][d][vo/gpu/opengl] [ 38] }
[   0.136][d][vo/gpu/opengl] [ 39] float rand(float x) { return fract(x * 1.0/41.0); }
[   0.136][d][vo/gpu/opengl] [ 40] vec4 average(float range, inout float h) {
[   0.136][d][vo/gpu/opengl] [ 41] float dist = rand(h) * range; h = permute(h);
[   0.136][d][vo/gpu/opengl] [ 42] float dir = rand(h) * 6.2831853; h = permute(h);
[   0.136][d][vo/gpu/opengl] [ 43] vec2 o = dist * vec2(cos(dir), sin(dir));
[   0.136][d][vo/gpu/opengl] [ 44] vec4 ref[4];
[   0.136][d][vo/gpu/opengl] [ 45] ref[0] = HOOKED_texOff(vec2( o.x, o.y));
[   0.136][d][vo/gpu/opengl] [ 46] ref[1] = HOOKED_texOff(vec2(-o.y, o.x));
[   0.136][d][vo/gpu/opengl] [ 47] ref[2] = HOOKED_texOff(vec2(-o.x, -o.y));
[   0.136][d][vo/gpu/opengl] [ 48] ref[3] = HOOKED_texOff(vec2( o.y, -o.x));
[   0.136][d][vo/gpu/opengl] [ 49] return (ref[0] + ref[1] + ref[2] + ref[3])*0.25;
[   0.136][d][vo/gpu/opengl] [ 50] }
[   0.136][d][vo/gpu/opengl] [ 51] void main() {
[   0.136][d][vo/gpu/opengl] [ 52] vec4 color = vec4(0.0, 0.0, 0.0, 1.0);
[   0.136][d][vo/gpu/opengl] [ 53] {
[   0.136][d][vo/gpu/opengl] [ 54] vec3 _m = vec3(HOOKED_pos, random) + vec3(1.0);
[   0.136][d][vo/gpu/opengl] [ 55] float h = permute(permute(permute(_m.x)+_m.y)+_m.z);
[   0.136][d][vo/gpu/opengl] [ 56] color = HOOKED_tex(HOOKED_pos);
[   0.136][d][vo/gpu/opengl] [ 57] vec4 avg, diff;
[   0.136][d][vo/gpu/opengl] [ 58] avg = average(16.000000, h);
[   0.136][d][vo/gpu/opengl] [ 59] diff = abs(color - avg);
[   0.136][d][vo/gpu/opengl] [ 60] color = mix(avg, color, bvec4(greaterThan(diff, vec4(0.001953))));
[   0.136][d][vo/gpu/opengl] [ 61] vec3 noise;
[   0.136][d][vo/gpu/opengl] [ 62] noise.x = rand(h); h = permute(h);
[   0.136][d][vo/gpu/opengl] [ 63] noise.y = rand(h); h = permute(h);
[   0.136][d][vo/gpu/opengl] [ 64] noise.z = rand(h); h = permute(h);
[   0.136][d][vo/gpu/opengl] [ 65] color.xyz += 0.005859 * (noise - vec3(0.5));
[   0.136][d][vo/gpu/opengl] [ 66] }
[   0.136][d][vo/gpu/opengl] [ 67] color.g = 0.000000;
[   0.136][d][vo/gpu/opengl] [ 68] color.b = 0.000000;
[   0.136][d][vo/gpu/opengl] [ 69] color.a = 1.000000;
[   0.136][d][vo/gpu/opengl] [ 70] out_color = color;
[   0.136][d][vo/gpu/opengl] [ 71] }
