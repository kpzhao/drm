[   0.176][d][vo/gpu/opengl] [ 56] color = HOOKED_tex(HOOKED_pos);
[   0.176][d][vo/gpu/opengl] [ 57] vec4 avg, diff;
[   0.176][d][vo/gpu/opengl] [ 58] avg = average(16.000000, h);
[   0.176][d][vo/gpu/opengl] [ 59] diff = abs(color - avg);
[   0.176][d][vo/gpu/opengl] [ 60] color = mix(avg, color, bvec4(greaterThan(diff, vec4(0.001953))));
[   0.176][d][vo/gpu/opengl] [ 61] vec3 noise;
[   0.176][d][vo/gpu/opengl] [ 62] noise.x = rand(h); h = permute(h);
