[   0.283][d][vo/gpu/opengl] [ 64] noise.z = rand(h); h = permute(h);
[   0.283][d][vo/gpu/opengl] [ 65] color.xyz += 0.005859 * (noise - vec3(0.5));
[   0.283][d][vo/gpu/opengl] [ 66] }
